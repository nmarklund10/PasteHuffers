<!DOCTYPE html>
<html>
<head>
  
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
  <title>PasteHuffer</title>
 <!--<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/dojo/1.10.4/dijit/themes/claro/claro.css" />-->
  <!--<script src="https://ajax.googleapis.com/ajax/libs/dojo/1.10.4/dojo/dojo.js"
            data-dojo-config="parseOnLoad: true, async: false"></script>-->
            <%= stylesheet_link_tag "themes/claro/claro" %>
            <%= stylesheet_link_tag "css/dgrid" %>
            <!--<%= javascript_include_tag '/assests/dojo/dojo', {"parseOnLoad" => true, "async" => false} %> -->
            <%= javascript_include_tag "dojo/dojo", :'data-dojo-config' => %Q(async: false,parseOnLoad:true, baseUrl: '/assets/dojo/', modulePaths: {modules: '/assets/modules', widgets: '/assets/widgets'})%>
  <script>
    require(['dojo/dom',
            'dojo/parser', 
            "dijit/layout/ContentPane",
            "dojo/request",
            "dijit/Dialog",
            "dijit/layout/TabContainer",
            "dgrid/Grid",
            'dijit/form/TextBox',
            'dijit/form/Button',
            'dojox/form/Uploader', 
            'dijit/form/Select',
            "dojo/domReady!",
        ], function (dom, parser, ContentPane, request, dialog, TabContainer, Grid) {
            window.dom = dom;
            window.parser = parser;
            window.TabContainer = TabContainer;
            window.ContentPane = ContentPane;
            window.request = request;
            window.DojoDialog = dialog;
            window.Grid = Grid;
            
            if( typeof window.setup !== "undefined")
            {
              window.setup();
            }
        });
  </script>
</head>
<body class="claro">

<%= yield %>

</body>
</html>
